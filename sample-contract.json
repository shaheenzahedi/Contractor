{
  "provider": "my provider name",
  "consumer": "my consumer body",
  "base_url": "https://api.hatchways.io/",
  "interactions": [
    {
      "method": "GET",
      "path": "/test/header",
      "status": 200,
      "request": {
        "headers": {
          "Content-Accepted": "Application/Json"
        },
        "body": {
          "login": "sample body login"
        },
        "headerRules": [
          {
            "name": "we",
            "type": "match",
            "value": "23"
          }
        ]
      },
      "response": {
        "body": {
          "login": "sample body login"
        },
        "headers": {
          "Content-Accepted": "Application/Json"
        }
      }
    },
    {
      "method": "DELETE",
      "path": "/assessment/blog/posts",
      "status": 405,
      "request": {
        "queryParamRules": [
          {
            "name": "loginStartsWith",
            "type": "doesNotMatch",
            "value": ".{0,2}"
          }
        ]
      },
      "response": {
        "body": {
          "wow": "This is perfect"
        },
        "bodyRules": [
          {
            "name": "error",
            "type": "match",
            "value": "The tag parameter is required"
          }
        ],
        "headerRules": [
          {
            "name": "Content-Type",
            "type": "regex",
            "value": "application/json(;\\s?charset=[\\w\\-]+)?"
          }
        ]
      }
    },
    {
      "method": "GET",
      "path": "/assessment/blog/posts",
      "status": 200,
      "request": {
        "params": {
          "tag": "tech"
        }
      },
      "response": {
        "body": {
          "id": 2908005593,
          "login": "user name",
          "content": "rIljtwdclWaGQfJNMBtm"
        }
      }
    },
    {
      "method": "POST",
      "path": "/assessment/blog/posts",
      "status": 200,
      "request": {
      },
      "response": {
        "body": {
          "login": "user name"
        }
      }
    }
  ]
}